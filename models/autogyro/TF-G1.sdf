<sdf version='1.5'>
  <model name='autogyro'>
   <pose>0 0 0.2 0 0 0</pose>
    <link name='base_link'>
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>-0.1 0 0 0 0 0</pose>
        <mass>4.9</mass>
        <inertia>
          <ixx>0.197563</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.1458929</iyy>
          <iyz>0</iyz>
          <izz>0.1477</izz>
        </inertia>
      </inertial>
      <collision name='base_link_collision'>
        <!-- Use with wheels -->
        <pose>-0.6 0 -0.12 0 0 0</pose>
        <geometry>
          <sphere><radius>0.02</radius></sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.05</mu>
              <mu2>0.01</mu2>
            </ode>
          </friction>
        </surface>

        <!-- Use without wheels
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>1.5 0.5 0.5</size>
          </box>
        </geometry>

        <surface>
          <friction>
            <ode>
              <mu>10</mu>
              <mu2>10</mu2>
            </ode>
          </friction>
        </surface>
      -->


      </collision>
      <visual name='base_link_visual'>
        <pose>0.22 0 0.07 1.5708 0 3.1416</pose>
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>model://autogyro/meshes/TF-G1-sim-body.dae</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Yellow</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>
      <self_collide>0</self_collide>
      <visual name='rear_wheel_visual'>
        <pose>-0.6 0 -0.12 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>0.02</radius>
          </sphere>
        </geometry>
      </visual>
    </link>
    <link name='imu_link'>
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.015</mass>
        <inertia>
          <ixx>1e-05</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-05</iyy>
          <iyz>0</iyz>
          <izz>1e-05</izz>
        </inertia>
      </inertial>
    </link>
    <joint name='imu_joint' type='revolute'>
      <child>imu_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>0</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

    <link name="left_front_wheel">
      <pose> 0.16 0.22 -0.095 1.5707 1.5707</pose>
       <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.01</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.01</iyy>
          <iyz>0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
            <cylinder>
              <radius>.05</radius>
              <length>.025</length>
            </cylinder>
          </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.1</mu>
              <mu2>0.01</mu2>
              <slip1>0</slip1>
              <slip2>0</slip2>
            </ode>
          </friction>
        </surface>

      </collision>
      <visual name="left_front_wheel_visual">
        <geometry>
            <cylinder>
              <radius>.05</radius>
              <length>.025</length>
            </cylinder>
          </geometry>
      </visual>
    </link>
    <joint name="left_front_wheel_joint" type="revolute">
      <pose>0 0 0 0 0 0 </pose>
      <child>left_front_wheel</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>1</use_parent_model_frame>
        <dynamics>
          <friction>0.01</friction>
          <damping>0</damping>
        </dynamics>
      </axis>
    </joint>

    <link name="right_front_wheel">
      <pose> 0.16 -0.22 -0.095 1.5707 1.5707</pose>
      <collision name="collision">
        <geometry>
            <cylinder>
              <radius>.05</radius>
              <length>.025</length>
            </cylinder>
          </geometry>
        <surface>
          <friction>
            <ode>
               <mu>0.1</mu>
              <mu2>0.01</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="right_front_wheel_visual">
        <geometry>
            <cylinder>
              <radius>.05</radius>
              <length>.025</length>
            </cylinder>
          </geometry>
      </visual>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.01</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.01</iyy>
          <iyz>0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="right_front_wheel_joint" type="revolute">
      <pose>0 0 0 0 0 0 </pose>
      <child>right_front_wheel</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>1</use_parent_model_frame>
        <dynamics>
          <friction>0.01</friction>
          <damping>0</damping>
        </dynamics>
      </axis>
    </joint>

